<div class="header">
    <div class="header-left">
        <% let home="/test-laboratoire"; if(USER_MENU_ACCES.includes("Tableau de bord") || USER_MENU_ACCES[0]=="All"){ home="/home";}%>
        <a href="<%=home%>" class="logo">
            <img src="data:image/jpeg;base64,<%=LOGO%>" width="35" height="35" alt=""> <span><%=appName%></span>
        </a>
    </div>
    <a id="toggle_btn" href="javascript:void(0);"><i class="fa fa-bars"></i></a>
    <a id="mobile_btn" class="mobile_btn float-left" href="#sidebar"><i class="fa fa-bars"></i></a>
    <ul class="nav user-menu float-right">
        <li class="nav-item dropdown d-none d-sm-block">
            <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown"><i class="fa fa-bell-o"></i>
                <span class="badge badge-pill bg-danger float-right" id="countNotifs"></span></a>
            <div class="dropdown-menu notifications">
                <div class="topnav-dropdown-header">
                    <span>Notifications</span>
                </div>
                <div class="drop-scroll">
                    <ul class="notification-list" id="notifications">
                    </ul>
                </div>
                <div class="topnav-dropdown-footer">
                    <a href="/notifications-list">Notifications</a>
                </div>
            </div>
        </li>
       
        <li class="nav-item dropdown has-arrow">
            <a href="#" class="dropdown-toggle nav-link user-link" data-toggle="dropdown">
                <span class="user-img">
                    <img class="rounded-circle" src="assets/img/user.jpg" width="24" alt="Admin">
                    <span class="status online"></span>
                </span>
                <span><%=USER_NAME%></span>
            </a>
            <div class="dropdown-menu">
                <a class="dropdown-item" href="/change-pwd">Changement de mot de passe</a>
                <a class="dropdown-item" href="/logout">Déconnexion</a>
            </div>
        </li>
    </ul>
    <div class="dropdown mobile-user-menu float-right">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i
                class="fa fa-ellipsis-v"></i></a>
        <div class="dropdown-menu dropdown-menu-right">
            <a class="dropdown-item" href="/change-pwd">Changement de mot de passe</a>
                <a class="dropdown-item" href="/logout">Déconnexion</a>
        </div>
    </div>
</div>