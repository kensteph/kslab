<!DOCTYPE html>
<html>

<head>
    <link rel="stylesheet" type="text/css" href="/assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
    <style>

    </style>
</head>

<body>
    
    <div class="row-header">
        <div class="logo">
            <img style="margin-left: 10px;" src="data:image/jpeg;base64,<%=LOGO%>" alt="img" width="150" height="150">
        </div>
        <div class="header-form">
            <p>
                <h3> <%=line1%> </h3>
                <h3><%=line2%></h3>
                <% if(line3 !=""){ %>
                 <%=line3%><br>
                <% } %>
                <% if(line4 !=""){ %>
                    <%=line4%><br>
                <% } %>
                <% if(line5 !=""){ %>
                    <%=line5%><br>
                <% } %>
                <% if(line6 !=""){ %>
                    <%=line6%><br><br>
                <% } %>
                <h3><hr></h3>
            </p>
        </div>
    </div>
    <div class="row-header">
        <div class="title-report"><%=pageTitle%></div>
    </div>
    <div class="row-report">
        <div class="col-sm-6">
            <div class="table-responsive">
                <table class="table table-striped custom-table datatable mb-0">
                    <thead>
                        <tr>
                            <th class="report">Examens</th>
                            <th class="report">Quantit√©</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((report) => {   %>
                        <tr>
                            <td class="report"><%=report.Examen%></td>
                            <td class="report"> <%=report.qty%></td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
   
</body>

</html>